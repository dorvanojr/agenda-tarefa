	<queryString>
		<![CDATA[select
       p.codigoBarra,
       p.nomeProduto,
       v.quantidade,
       p.precoVenda,
       v.valorTotal
 from estoque.vendas v
      inner join
      estoque.produto p on p.idProduto =  v.idProduto
